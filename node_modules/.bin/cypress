#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vukan/Ventage/vantage-ui-templates/node_modules/cypress/bin/node_modules:/home/vukan/Ventage/vantage-ui-templates/node_modules/cypress/node_modules:/home/vukan/Ventage/vantage-ui-templates/node_modules:/home/vukan/Ventage/node_modules:/home/vukan/node_modules:/home/node_modules:/node_modules:/home/vukan/Ventage/vantage-ui-templates/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vukan/Ventage/vantage-ui-templates/node_modules/cypress/bin/node_modules:/home/vukan/Ventage/vantage-ui-templates/node_modules/cypress/node_modules:/home/vukan/Ventage/vantage-ui-templates/node_modules:/home/vukan/Ventage/node_modules:/home/vukan/node_modules:/home/node_modules:/node_modules:/home/vukan/Ventage/vantage-ui-templates/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cypress/bin/cypress" "$@"
else
  exec node  "$basedir/../cypress/bin/cypress" "$@"
fi
